{
    "manifest_version": 3,
    "name": "Wonder Salesforce Formula Editor ðŸª„",
    "description": "Your Salesforce Formula Editor. Format, validate & enjoy a stunning UI. Perfect formulas effortlessly ðŸª„",
    "version": "0.0.1",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "dist/scripts/content-script.min.js"
            ],
            "matches": [
                "*://*.salesforce.com/**"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "dist/ui/editor.min.js"
            ],
            "matches": [
                "https://*.salesforce.com/*"
            ],
            "use_dynamic_url": true
        },
        {
            "resources": [
                "dist/scripts/insert-in-object-manager/index.min.js"
            ],
            "matches": [
                "https://*.salesforce.com/*"
            ],
            "use_dynamic_url": true
        }
    ]
}